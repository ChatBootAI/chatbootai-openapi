                                    <skipOperationExample>${openapi-generator-plugin-skipOperationExample}</skipOperationExample>
                                    <inlineSchemaNameMappings>chatRequest_messages_inner=chatRequestMessage,chatResponse_choices_inner=chatResponseChoice</inlineSchemaNameMappings>
                                    <configOptions>
